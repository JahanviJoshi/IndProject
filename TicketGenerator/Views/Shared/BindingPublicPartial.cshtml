
@model TicketGenerator.Models.Tickect


<html>
<head>
    <meta name="viewport" content="width=device-width"/>

    <title>Index</title>
</head>
<body>
    <h4>Customers</h4>
    <hr/>
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>TicketNo</th>
            <th>Title</th>
            <th>Message</th>
            <th>DateAndTime</th>
            <th>Status</th>
            <th>AssignTo</th>
        </tr>
        @foreach (var item in @ViewBag.tickect)
        {
            <tr>
                <td>@item.TicketNo</td>
                <td>@item.Title</td>
                <td>@item.Message</td>
                <td>@item.DateAndTime</td>
                 <td>@item.Status</td>
                  <td>@item.AssignTo</td>
            </tr>
        }
    </table>
</body>
</html>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
